function [isg,loi] = ISG(G)
%ISG Get the grid coordinates within the same grid.

    isg = {};
    index = 1:length(G);
    loi = [];
    for i = 1:length(G)
        tmp = [];
        tmp = [tmp,index(ismember(G,G(i,:),'rows'))];
        loi = [loi;length(tmp)];
        isg = [isg;tmp];
    end
end

